import java.util.Scanner;
public class Exercicio4 {
	public static void main(String[] args) {
		Scanner in = new Scanner (System.in);
		int a[], i;
		double v;
		a=new int [10];
		v=0;
		for (i=0;i<10;i++) {
			System.out.println("Coloque o "+(i+1)+ "° valor");
			a[i]=in.nextInt();
		     v=v+a[i];
		     
		}
		v = v/10;
		System.out.println("a média é "+v);
	}
}
